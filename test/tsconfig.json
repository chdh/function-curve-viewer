{
   "compilerOptions": {
      "allowUnreachableCode": false,
      "alwaysStrict": true,
      "baseUrl": ".",
      "lib": ["ES6", "DOM"],
      "module": "ES6",
      "moduleResolution": "node",
      "noFallthroughCasesInSwitch": true,
      "noImplicitAny": true,
      "noImplicitReturns": true,
      "noImplicitThis": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "outDir": "tempBuild",
      "paths": {
         "function-curve-viewer": ["../dist/FunctionCurveViewer.d.ts"]
      },
      "sourceMap": true,
      "strictFunctionTypes": true,
      "strictNullChecks": true,
      "target": "ES6"
   },
   "files": ["app.ts"]
}
